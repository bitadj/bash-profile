{
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "editor.fontFamily": "bitadj code",
  "editor.fontSize": 12,
  "editor.fontLigatures": true,
  "editor.wordWrap": "off",
  "window.zoomLevel": 0,
  "workbench.editor.showTabs": true,
  "explorer.openEditors.visible": 10,
  "editor.cursorStyle": "line",
  "workbench.activityBar.visible": true,
  "workbench.statusBar.visible": true,
  "files.associations": {
    ".sequelizerc": "javascript",
    "sequelizerc": "javascript",
    "*.sample": "javascript"
  },
  "editor.selectionHighlight": false,
  "editor.renderLineHighlight": "none",
  "editor.snippetSuggestions": "top",
  "beautify.language": {
    "js": {
      "type": [
        "javascript",
        "json",
        "javascriptreact"
      ],
      "filename": [
        ".jshintrc",
        ".jsbeautify"
      ]
    },
    "css": [
      "css",
      "scss"
    ],
    "html": [
      "htm",
      "html"
    ]
  },
  "editor.minimap.enabled": false,
  "workbench.iconTheme": "vscode-icons",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      // {
      //   "scope": [
      //     "string.quoted.single.js.jsx",
      //     "string.quoted.single.js",
      //     "string.quoted.double.js.jsx",
      //   ],
      //   "settings": {
      //     "foreground": "#adac57",
      //   }
      // },
      {
        "scope": [
            "variable.language.js",
            "variable.language.jsx",
        ],
        "settings": {
            "foreground": "#bf616a",
        }
      },
      {
        "scope": [
            "variable.language.this.js.jsx",

        ],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#f593f8",
        }
      },
      {
        "scope": [
            "comment",
        ],
        "settings": {
          "fontStyle": "italic",
        }
      },
      {
        "scope": [
            "keyword.operator",
            "storage.type",
        ],
        "settings": {
          "fontStyle": "bold",
        }
      },
    ]
  },
  "terminal.external.osxExec": "iTerm2.app",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.fontFamily": "Monaco",
  "search.location": "sidebar",
  "editor.wordWrapColumn": 120,
  "files.trimTrailingWhitespace": true,
  "editor.renderWhitespace": "boundary",
  "npm.enableScriptExplorer": true,
  "gitlens.gitExplorer.files.layout": "tree",
  "gitlens.codeLens.scopes": [
    "document",
  ],
  "window.title": "${rootName}",
  "editor.tabCompletion": true,
  "emmet.triggerExpansionOnTab": true,
  "local-history.daysLimit": 10,
  "workbench.statusBar.feedback.visible": false,
  "gitlens.statusBar.alignment": "right",
  "gitlens.gitExplorer.view": "history",
  "gitlens.advanced.messages": {
    "suppressShowKeyBindingsNotice": true
  },
  "gitlens.gitExplorer.enabled": false,
  "gitlens.historyExplorer.enabled": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    ".history": true,
    "package-lock.json": true,
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/dist/**": true,
  },
  "local-history.exclude": [
    "**/.history/**",
    "**/.vscode/**",
    "**/node_modules/**",
    "**/typings/**",
    "**/out/**",
    "**/Code/User/**",
    "**/dist/**",
  ],
  "[json]": {},
}
