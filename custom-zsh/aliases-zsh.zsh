
# Unix aliases
alias cl='clear'
alias ll='ls -a'

# .zsh
alias nanobash='code ~/.zshrc'
alias nanoalias='code $ZSH_CUSTOM'
alias nanodir='code $ZSH_CUSTOM'
alias sourcebash='source $ZSH/oh-my-zsh.sh'

alias zshrc="code ~/.zshrc"

# alias-ception
alias nb='nanobash'
alias na='nanoalias'
alias nd='nanodir'
alias sb='sourcebash'
alias gt='gitalias'

# display aliases
alias al='dog $ZSH_CUSTOM/aliases-zsh.zsh'
alias diral='dog $ZSH_CUSTOM/diraliases.zsh'
alias gital='dog $ZSH_CUSTOM/git-aliases.zsh'
alias uu='dog $ZSH_CUSTOM/diraliases-uu.zsh'
alias aws='dog $ZSH_CUSTOM/local-alias.zsh'

# fun and pointless
alias myip='curl ifconfig.me'
alias dog='pygmentize -g '
alias chr='open -a "Google Chrome"'
